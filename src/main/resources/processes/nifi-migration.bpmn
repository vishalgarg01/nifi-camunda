<?xml version="1.0" encoding="UTF-8"?>
<bpmn:definitions xmlns:bpmn="http://www.omg.org/spec/BPMN/20100524/MODEL"
                  xmlns:bpmndi="http://www.omg.org/spec/BPMN/20100524/DI"
                  xmlns:dc="http://www.omg.org/spec/DD/20100524/DC"
                  xmlns:di="http://www.omg.org/spec/DD/20100524/DI"
                  xmlns:camunda="http://camunda.org/schema/1.0/bpmn"
                  targetNamespace="http://emigran.com/camunda/nifi">
  <bpmn:process id="nifiMigrationProcess" name="NiFi Dataflow Migration" isExecutable="true">
    <bpmn:startEvent id="StartEvent_Migration" name="Start">
      <bpmn:outgoing>Flow_Start_To_Migrate</bpmn:outgoing>
    </bpmn:startEvent>

    <bpmn:serviceTask id="ServiceTask_Migrate" name="Run NiFi Migration"
                      camunda:delegateExpression="${nifiDataflowMigrationDelegate}">
      <bpmn:incoming>Flow_Start_To_Migrate</bpmn:incoming>
      <bpmn:outgoing>Flow_Migrate_To_End</bpmn:outgoing>
    </bpmn:serviceTask>

    <bpmn:endEvent id="EndEvent_Migration" name="Done">
      <bpmn:incoming>Flow_Migrate_To_End</bpmn:incoming>
    </bpmn:endEvent>

    <bpmn:sequenceFlow id="Flow_Start_To_Migrate" sourceRef="StartEvent_Migration" targetRef="ServiceTask_Migrate"/>
    <bpmn:sequenceFlow id="Flow_Migrate_To_End" sourceRef="ServiceTask_Migrate" targetRef="EndEvent_Migration"/>
  </bpmn:process>

  <bpmndi:BPMNDiagram id="BPMNDiagram_NiFiMigration">
    <bpmndi:BPMNPlane id="BPMNPlane_NiFiMigration" bpmnElement="nifiMigrationProcess">
      <bpmndi:BPMNShape id="StartEvent_Migration_di" bpmnElement="StartEvent_Migration">
        <dc:Bounds x="180" y="102" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="ServiceTask_Migrate_di" bpmnElement="ServiceTask_Migrate">
        <dc:Bounds x="270" y="80" width="160" height="80"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNShape id="EndEvent_Migration_di" bpmnElement="EndEvent_Migration">
        <dc:Bounds x="480" y="102" width="36" height="36"/>
      </bpmndi:BPMNShape>
      <bpmndi:BPMNEdge id="Flow_Start_To_Migrate_di" bpmnElement="Flow_Start_To_Migrate">
        <di:waypoint x="216" y="120"/>
        <di:waypoint x="270" y="120"/>
      </bpmndi:BPMNEdge>
      <bpmndi:BPMNEdge id="Flow_Migrate_To_End_di" bpmnElement="Flow_Migrate_To_End">
        <di:waypoint x="430" y="120"/>
        <di:waypoint x="480" y="120"/>
      </bpmndi:BPMNEdge>
    </bpmndi:BPMNPlane>
  </bpmndi:BPMNDiagram>
</bpmn:definitions>